<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<display-name>pages/videogame/videogame_list.xhtml</display-name>
		<from-view-id>/pages/videogame/videogame_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>form</from-outcome>
			<to-view-id>/pages/videogame/videogame_form.xhtml</to-view-id>
			
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>pages/videogame/videogame_form.xhtml</display-name>
		<from-view-id>/pages/videogame/videogame_form.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>list</from-outcome>
			<to-view-id>/pages/videogame/videogame_list.xhtml</to-view-id>
			
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<display-name>pages/associado/associado_list.xhtml</display-name>
		<from-view-id>/pages/associado/associado_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>form</from-outcome>
			<to-view-id>/pages/associado/associado_form.xhtml</to-view-id>
			
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>pages/associado/associado_form.xhtml</display-name>
		<from-view-id>/pages/associado/associado_form.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>list</from-outcome>
			<to-view-id>/pages/associado/associado_list.xhtml</to-view-id>
			
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
